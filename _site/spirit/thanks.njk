---
title: 'Thanks!'
layout: 'layouts/default.njk'
---

{%- from 'components/heading.njk' import heading -%}
{%- from 'components/divider.njk' import divider -%}

<section>
  <div>{{ divider() }}</div>
  <article class="mx-auto py-8 px-4 bg-white relative lg:(py-24)">
    <div class="mx-auto max-w-7xl">
      <div class="mx-auto py-12 max-w-2xl lg:(mx-0)">
        {%- call heading({}) %}
          {{ spirit.donated.heading }}
        {%- endcall %}
        <p class="text-2xl leading-normal">{{ spirit.donated.lead }}</p>
      </div>
    </div>
  </article>
</section>
